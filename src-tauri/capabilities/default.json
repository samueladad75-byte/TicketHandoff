{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "sql:allow-load",
    "stronghold:default",
    "stronghold:allow-load-client",
    "stronghold:allow-create-client",
    "stronghold:allow-save-secret",
    "stronghold:allow-remove-secret",
    "stronghold:allow-execute-procedure",
    "fs:default",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:allow-exists",
    "fs:scope-app-index",
    "fs:scope-appcache-index",
    "fs:scope-appconfig-index",
    "fs:scope-appdata-index",
    "fs:scope-applocaldata-index",
    "fs:scope-applog-index",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
